
public class Administrador extends Autenticavel {
	private int senha;
	
	public double getBonificacao() {
		return 400;
	}
	
	public void setSenha(int senha) {
		this.senha = senha;
	}
	
	public boolean autentica(int senha) {
		if(this.senha == senha) {
			return true;
		} else {
			return false;
		}
	}
}
